               <RadioButton fx:id="rbSwitchAuto" layoutX="113.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleRadioButtonsSwitch" prefHeight="18.0" prefWidth="79.0" text="Auto" textFill="AQUA" toggleGroup="$s" />
